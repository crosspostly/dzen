    "natural": "^6.11.0",
    "piexifjs": "^1.0.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "uuid": "^13.0.0"
