{
  "name": "zenmaster-ai-article-generator-for-dzen",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "generate": "node --import tsx cli.ts generate",
    "generate:v2": "node --import tsx cli.ts generate:v2",
    "factory": "node --import tsx cli.ts factory",
    "generate:all-dzen": "node --import tsx cli.ts generate:all-dzen",
    "list-dzen-channels": "node --import tsx cli.ts list-dzen-channels",
